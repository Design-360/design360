<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title"> <%= message.message_sender.email %></h3>
  </div>
  <div class="panel-body">
    <%= time_ago_in_words(message.created_at) %> ago<br>
    <%= message.content %> <br>
    <%= link_to message.document_file_name,message.document.url if message.document.exists? %>
  </div>
</div>